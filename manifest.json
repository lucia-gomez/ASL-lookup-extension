{
  "manifest_version": 2,
  "name": "ASL Search",
  "version": "1.0",
  "description": "Search for a sign in ASL",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}